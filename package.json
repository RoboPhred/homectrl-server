{
  "name": "root",
  "private": true,
  "scripts": {
    "__core": "exit 1",
    "clean:td": "cd packages/wutwot-td && npm run clean",
    "build:td": "cd packages/wutwot-td && npm run build",
    "clean:core": "cd packages/wutwot-core && npm run clean",
    "build:core": "cd packages/wutwot-core && npm run build",
    "__servients": "exit 1",
    "clean:binding-express": "cd packages/wutwot-binding-express && npm run clean",
    "build:binding-express": "cd packages/wutwot-binding-express && npm run build",
    "clean:servient-express": "cd packages/wutwot-servient-express && npm run clean",
    "build:servient-express": "cd packages/wutwot-servient-express && npm run build",
    "clean:webui-express": "cd packages/wutwot-webui-express && npm run clean",
    "build:webui-express": "cd packages/wutwot-webui-express && npm run build",
    "__plugins": "exit 1",
    "clean:model": "cd packages/wutwot-model && npm run clean",
    "build:model": "cd packages/wutwot-model && npm run build",
    "clean:scenes": "cd packages/wutwot-scenes && npm run clean",
    "build:scenes": "cd packages/wutwot-scenes && npm run build",
    "clean:zwave": "cd packages/wutwot-zwave && npm run clean",
    "build:zwave": "cd packages/wutwot-zwave && npm run build",
    "__server": "exit 1",
    "clean:server": "cd packages/wutwot-server && npm run clean",
    "build:server": "cd packages/wutwot-server && npm run build",
    "__macros": "exit 1",
    "clean:prereqs": "npm-run-all -p clean:td clean:core",
    "build:prereqs": "npm-run-all build:td build:core",
    "clean:plugins": "npm-run-all -p clean:model clean:scenes clean:zwave clean:binding-express clean:servient-express clean:webui-express",
    "build:plugins": "npm-run-all -p build:model build:scenes build:zwave build:binding-express build:servient-express build:webui-express",
    "clean": "npm-run-all -p clean:prereqs clean:plugins",
    "build": "npm-run-all build:prereqs build:plugins build:server",
    "__development": "exit 1",
    "start": "cd packages/wutwot-server && npm run start",
    "start:client": "cd packages/wutwot-explorer && npm run start -- --port=6060"
  },
  "workspaces": [
    "./packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RoboPhred/wutwot.git"
  },
  "keywords": [
    "w3c-td",
    "iot",
    "zwave"
  ],
  "author": "William Matthews",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RoboPhred/wutwot/issues"
  },
  "homepage": "https://github.com/RoboPhred/wutwot#readme",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5"
  }
}
