{
  "$schema": "http://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6"],
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "stripInternal": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "sourceMap": true
  }
}
